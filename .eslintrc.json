{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"globals": {
		"discordClient": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 12
	},
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-prototype-builtins": "off",
		"no-console": "warn",
		"prefer-const": "error",
		"no-useless-concat": "error",
		"prefer-template": "error"
	}
}
